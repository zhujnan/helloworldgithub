package com.day01.demo2;

import java.util.Scanner;

public class Demo02 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		
		switch (a) {
			case 1:
					System.out.println("周一");
			case 2:
					System.out.println("周二");
			case 3:
					System.out.println("周三");
				break;

		default:
			break;
		}
	}
}
