public class Consumer extends Thread{
	private int neednum; ///Ïû·Ñ²úÆ·µÄÊýÁ¿
	private Godown godown; //ï¿½Ö¿ï¿½
	public Consumer(int neednum, 
			Godown godown) {
		this.neednum = neednum;
		this.godown = godown;
	}
	public void run() {
		godown.consume(neednum);//ï¿½ï¿½ï¿½ï¿½Ö¸ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ï¿½ ï¿½ï¿½Æ·
	}
}
