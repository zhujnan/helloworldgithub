package com.day01.demo2;

public class Demo03 {
	public static void main(String[] args) {
		/**
		 * for(int i = 0;i<length:i++){
		 * 		程序循环体
		 * 	}
		 */
		int sum = 0;
		for(int i =1;i<100;i+=2) {
			sum+=i;
		}
		System.out.println(sum);//1050
		
		/**
		 * 打印99乘法表
		 */
		for(int i =1;i<9;i++) {
			for(int j =1;j<=i;j++) {
				System.out.print(i+"*"+j+"="+i*j);
			}
			System.out.println();
		}
		
	}
}
