package com.day10.dem;

public class Test extends Thread{
		static int n = 0;
		private int startNum=0;
		public Test(int sn) {
			startNum = sn;
		}
		public static synchronized void addSum(int num) {
			n = n + num;
		}
		public static void main(String[] args) {
			Thread[] thList = new Thread[10];
			
		}
}
